<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="TRACE">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %-5level %logger{36}: %ms"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root levels="error">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>
